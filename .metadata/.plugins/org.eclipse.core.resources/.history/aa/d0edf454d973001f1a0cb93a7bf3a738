#include "base.h"
#include <stdio.h>
#include <string.h>

typedef unsigned char uint8_t;
typedef unsigned short uint16_t;

void genline(byte n, byte offset) {
    word org = 0xF820;
    if (n <= 8 - offset) {
        org = 0xFF >> offset;
    }
}

int main() {
	memset(0xD000,0x00,0x15D3);
	memset(0xf800,0x00,0x800);
	while (1) {
		genline(4, 3);
	}
    return 0;
}
